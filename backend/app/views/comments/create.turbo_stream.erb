<%= turbo_stream.update "comment-post-#{@post.id.to_s}" do %>
    <%= render partial: "posts/comment_list", locals: { post: @post } %>
<% end %>

<%= turbo_stream.update "comment-form-post-#{@post.id.to_s}" do %>
    <%= render partial: 'comments/comment_form', locals: { post: @post } %>
<% end %>



